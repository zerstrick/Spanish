msgstr "<link=\"BRICK\">Ladrillo</link>"

#. STRINGS.ELEMENTS.BRINE.DESC
msgctxt "STRINGS.ELEMENTS.BRINE.DESC"
msgid ""
"Brine is a natural, highly concentrated solution of <link=\"SALT\">Salt</"
"link> dissolved in <link=\"WATER\">Water</link>.\n"
"\n"
"It can be used in desalination processes, separating out useable salt."
msgstr ""
"La Salmuera es una solución natural altamente concentrada de <link=\"SALT"
"\">Sal</link> disuelta en <link=\"WATER\">Agua</link>.\n"
"\n"
"Se puede utilizar en procesos de desalinización, separando la sal utilizable."

#. STRINGS.ELEMENTS.BRINE.NAME
msgctxt "STRINGS.ELEMENTS.BRINE.NAME"
msgid "<link=\"BRINE\">Brine</link>"
msgstr "<link=\"BRINE\">Salmuera</link>"

#. STRINGS.ELEMENTS.BRINEICE.DESC
msgctxt "STRINGS.ELEMENTS.BRINEICE.DESC"
msgid ""
"Brine is a natural, highly concentrated solution of <link=\"SALT\">Salt</"
"link> dissolved in <link=\"WATER\">Water</link>.\n"
"\n"
"It can be used in desalination processes, separating out useable salt."
msgstr ""
"La Salmuera es una solución natural altamente concentrada de <link=\"SALT"
"\">Sal</link> disuelta en <link=\"WATER\">Agua</link>.\n"
"\n"
